/* pages/wishDesign/wishDesign.wxss */
page {
  min-height: 100%;
  background: #f4f4f4;
}

.container {
}

.select-form {
  width: 750rpx;
  position: relative;
}

.select-form .background>image:nth-child(1) {
  width: 750rpx;
  height: 110rpx;
  display: block;
}

.select-form .background>image:nth-child(2) {
  width: 750rpx;
  height: 358rpx;
  display: block;
}

.select-form .background>image:nth-child(3) {
  width: 750rpx;
  height: 110rpx;
  display: block;
}

.wenli {
  position: absolute;
  background: url("https://fjgz360.cn/chuxian/webPage/images/background/ceshi_title@3x.png") no-repeat center;
  background-size: 276rpx;
  width: 276rpx;
  height: 56rpx;
  top: 36rpx;
  right: 236rpx;
  text-align: center;
  font-size: 28rpx;
  line-height: 56rpx;
  color: #f7ce2c;
  letter-spacing: 0;
}

.user-data {
  position: absolute;
  top: 107rpx;
  display: flex;
  width: 750rpx;
}

.user-data>view {
  flex: 1;
  text-align: center;
}

.user-data>view>view {
  margin-top: 8rpx;
  height: 80rpx;
}

.user-data .data-title {
  font-size: 20rpx;
  color: #6a7088;
  height: 28rpx;
  line-height: 1;
}

.score-detail .score-number {
  height: 80rpx;
  line-height: 80rpx;
  width: 150rpx;
  font-size: 40rpx;
  color: #252628;
  background: url("https://fjgz360.cn/chuxian/webPage/images/icon/gaokaochengji@3x.png") no-repeat center;
  background-size: 150rpx;
  margin: 0 auto;
}

.ranking-detail  .ranking-number {
  height: 80rpx;
  line-height: 80rpx;
  width: 150rpx;
  font-size: 40rpx;
  color: #252628;
  background: url("https://fjgz360.cn/chuxian/webPage/images/icon/quanshengpaiming@3x.png") no-repeat center;
  background-size: 150rpx;
  margin: 0 auto;
}

.search-btn {
  position: absolute;
  bottom: 40rpx;
  right: 226rpx;
  height: 60rpx;
  width: 290rpx;
  font-size: 28rpx;
  color: #252628;
  background: #f7ce2c;
  border-radius: 200rpx;
  text-align: center;
  line-height: 60rpx;
}

.school-item {
  position: relative;
  background: #fff;
  height: 250rpx;
  width: 700rpx;
  margin: 0 25rpx 30rpx 25rpx;
}

.school-item .school-title {
  display: flex;
  height: 50rpx;
  line-height: 50rpx;
  border-bottom: 2rpx dashed #f4f4f4;
}

.school-item .school-title .grade {
  width: 242rpx;
  text-align: center;
  opacity: 0.7;
  font-size: 24rpx;
  color: #5b6369;
}

.school-item .school-title .type {
  flex: 1;
}

.school-item .school-title .type image {
  height: 28rpx;
  margin-top: 12rpx;
  margin-right: 20rpx;
}

.school-item .school-title .type .img985 {
  width: 48rpx;
}

.school-item .school-title .type .img211 {
  width: 48rpx;
}

.school-item .school-title .type .imgdouble {
  width: 80rpx;
}

.area {
  display: flex;
  width: 204rpx;
  opacity: 0.7;
  font-size: 24rpx;
  color: #5b6369;
}

.area .province {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-right: 10rpx;
  width: 72rpx;
}

.area .city {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-left: 10rpx;
  width: 72rpx;
}

.school-name {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 50rpx;
  color: #252628;
  height: 72rpx;
  line-height: 72rpx;
  margin: 0rpx 20rpx 0 20rpx;
  text-align: center;
}

.school-info {
  background-image: linear-gradient(-180deg, #fff9b8 0%, #fff082 100%);
  height: 60rpx;
  display: flex;
  font-size: 28rpx;
  color: #252628;
  line-height: 60rpx;
  margin-top: 20rpx;
}

.school-info .school-ranking {
  margin-left: 20rpx;
  flex: 1;
}

.school-info .school-manage {
  margin-right: 20rpx;
  display: flex;
}

.school-info text:first-child {
  margin-right: 20rpx;
}

.school-manage text:first-child {
  margin-right: 0;
}

.school-manage text:last-child {
  min-width: 84rpx;
}

.batch-list {
  position: absolute;
  top: 280rpx;
  left: 100rpx;
  display: flex;
}

.batch-title {
  font-size: 20rpx;
  color: #6a7088;
  line-height: 40rpx;
}

.batch-item {
  font-size: 24rpx;
  margin-left: 30rpx;
  height: 40rpx;
  line-height: 40rpx;
  width: 110rpx;
  color: #f7ce2c;
  text-align: center;
}

.batch-item.on {
  background: rgba(247, 206, 44, 0.10);
  border: 2rpx solid #f7ce2c;
  border-radius: 61rpx;
}

.rank-input input {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 40rpx;
  text-align: center;
  color: #252628;
  background: url("https://fjgz360.cn/chuxian/webPage/images/icon/quanshengpaiming@3x.png") no-repeat center;
  background-size: 150rpx;
}

.wenli-list {
  position: absolute;
  top: 36rpx;
  left: 234rpx;
  display: flex;
  font-size: 28rpx;
  color: #f7ce2c;
}

.wenli-list .wenli-item {
  border: 2rpx solid #f7ce2c;
  border-radius: 61rpx;
  width: 136rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
}

.wenli-list .wenli-item:last-child {
  margin-left: 18rpx;
}

.wenli-list .wenli-item.on {
  color: #fff;
  background: #f7ce2c;
}

.school-rate {
  opacity: 0.7;
  font-size: 24rpx;
  height: 34rpx;
  line-height: 34rpx;
  color: #5b6369;
  text-align: center;
  margin-top: 20rpx;
}

.recommend-type {
  position: absolute;
  top: 50rpx;
  right: 26rpx;
}

.recommend-type image {
  width: 120rpx;
  height: 120rpx;
}

.priority-item {
  position: absolute;
  width: 146rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  bottom: 16rpx;
  font-size: 28rpx;
  color: #252628;
}

.priority-item.on {
  background: url("https://fjgz360.cn/chuxian/webPage/images/icon/youxianji@3x.png") no-repeat center;
  background-size: 146rpx;
}

.priority-school {
  left: 54rpx;
}

.priority-subject {
  right: 54rpx;
}

.intention-area {
  position: absolute;
  top: 344rpx;
  left: 100rpx;
  display: flex;
  width: 572rpx;
    padding-right: 58rpx;
  box-sizing:  border-box;
    background: url("https://fjgz360.cn/chuxian/webPage/images/icon/right@3x.png") no-repeat right;
  background-size: 32rpx;
}

.intention-title {
  font-size: 20rpx;
  color: #6a7088;
  line-height: 40rpx;
  margin-right: 20rpx;
}

.intention-subject {
  position: absolute;
  top: 410rpx;
  left: 100rpx;
  display: flex;
  width: 572rpx;
  height: 44rpx;
  padding-right: 58rpx;
  box-sizing:  border-box;
    background: url("https://fjgz360.cn/chuxian/webPage/images/icon/right@3x.png") no-repeat right;
  background-size: 32rpx;
  overflow: hidden;
  flex-wrap:wrap;
}

.intention-subject .intention-item{
background: rgba(247,206,44,0.10);
border: 2rpx solid #F7CE2C;
border-radius: 56rpx;
font-size:24rpx;
margin-left:10rpx;
height:40rpx;
line-height:40rpx;
color:#f7ce2c;
text-align:center;
padding: 0 20rpx;


}
/* 专业选择 */

.mask {
  background: rgba(0, 0, 0, 0.5);
  height: 100%;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  display: none;
}

.mask.open {
  display: block;
}

.subject-list {
  width: 710rpx;
  height: 646rpx;
  background: #fff;
  position: fixed;
  border-radius: 10rpx;
  bottom: 0;
  margin: 20rpx;
}

.list-title {
  display: flex;
  padding: 0 30rpx;
  margin-top: 20rpx;
}

.list-title image {
  width: 74rpx;
  height: 74rpx;
}

.list-title text {
  font-size: 36rpx;
  color: #93969c;
  text-align: center;
  line-height: 74rpx;
  flex: 1;
}

.list-title .list-close {
  background: url("https://fjgz360.cn/chuxian/webPage/images/icon/dialog-close@3x.png") no-repeat center;
  background-size: 74rpx;
}

.list-title .list-submit {
  background: url("https://fjgz360.cn/chuxian/webPage/images/icon/dialog-submit@3x.png") no-repeat center;
  background-size: 74rpx;
}

.subject-list .list {
  height: 528rpx;
  width: 630rpx;
  padding: 12rpx 40rpx;

}
.scroll-list{
  display: flex;
  flex-wrap: wrap;
}
.subject-list .subject-item {
  background: rgba(247, 206, 44, 0.1);
  border-radius: 36rpx;
  border: 1rpx solid rgba(247, 206, 44, 0.1);
  font-size: 32rpx;
  color: #f7ce2c;
  height: 50rpx;
  line-height: 50rpx;
  padding: 0 20rpx;
  margin-bottom: 30rpx; 
  margin-right: 30rpx;
}
.subject-list .subject-item.on{
  border: 1rpx solid #F7CE2C;
}